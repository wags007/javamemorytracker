modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start Flask App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Status Check"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Commit"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Remote Check"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Status"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Add and Commit"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Push"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Update Git Remote"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Add All"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Config"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Add"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Commit Auto-Push"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Commit .replit"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Add .replit"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Status Final"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Git Add Auto-Push"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Run k6 Load Test"

[[workflows.workflow]]
name = "Start Flask App"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5001

[[workflows.workflow]]
name = "Git Status Check"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git status"

[[workflows.workflow]]
name = "Git Commit"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git commit -m \"Configure automatic push to remote repository\""

[[workflows.workflow]]
name = "Git Remote Check"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git remote -v"

[[workflows.workflow]]
name = "Git Status"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git status"

[[workflows.workflow]]
name = "Git Add and Commit"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git add README.md && git commit -m 'Update README.md with auto-push feature'"

[[workflows.workflow]]
name = "Git Push"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git push origin main"

[[workflows.workflow]]
name = "Update Git Remote"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git remote set-url origin https://${GITHUB_TOKEN}@github.com/wags007/javamemorytracker.git"

[[workflows.workflow]]
name = "Git Add All"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git add ."

[[workflows.workflow]]
name = "Git Config"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git config --global user.email \"noreply@example.com\" && git config --global user.name \"Replit User\""

[[workflows.workflow]]
name = "Git Add"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git add .github/workflows/auto-push.yml"

[[workflows.workflow]]
name = "Git Commit Auto-Push"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git commit -m 'Configure automatic push to remote repository'"

[[workflows.workflow]]
name = "Git Commit .replit"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git commit -m 'Update .replit configuration for automatic push'"

[[workflows.workflow]]
name = "Git Add .replit"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git add .replit"

[[workflows.workflow]]
name = "Git Status Final"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git status"

[[workflows.workflow]]
name = "Git Add Auto-Push"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git add .github/workflows/auto-push.yml"

[[workflows.workflow]]
name = "Run k6 Load Test"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "k6 run load_test.js"

[deployment]
run = ["sh", "-c", "python main.py"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3000
